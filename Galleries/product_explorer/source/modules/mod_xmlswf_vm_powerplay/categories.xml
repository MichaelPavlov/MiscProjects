	<btn label="Industrial" color="0x111111" path="xml/imageGallery/categories/industrial.xml" />
    <btn label="Winter" color="0x111111" path="xml/imageGallery/categories/winter.xml" />
	<btn label="Landscape" color="0x111111" path="xml/imageGallery/categories/landscape.xml" />
    <btn label="Nature" color="0x111111" path="xml/imageGallery/categories/nature.xml" />
	<btn label="Christmas" color="0x111111" path="xml/imageGallery/categories/christmas.xml" />
    <btn label="City" color="0x111111" path="xml/imageGallery/categories/city.xml" />
</buttons>
</xml>


